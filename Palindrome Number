#include <stdio.h>
void main()
{
  int n, r, rev = 0 ,m;

  printf("Enter a number = ");
  scanf("%d", &n);
 m=n;
  while (n != 0)
  {
    r = n % 10;
    rev = rev * 10 + r;
    n = n/10;
  }
 if(m==rev)    
 printf("Given Number is Palindrome number ");   
 else    
 printf("Not a palindrome Number");  
}
